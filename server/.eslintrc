{
  "root": true,
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "parser": "babel-eslint",
  "rules": {
    "import/prefer-default-export": 0,
    "linebreak-style": 0,
    "object-curly-newline": 0,
    "arrow-parens": 0,
    "no-param-reassign": 0
  }
}
